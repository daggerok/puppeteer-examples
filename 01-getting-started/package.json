{
  "scripts": {
    "clean": "rimraf -rf ./dist",
    "start": "parcel --no-cache ./src/index.html",
    "test": "jest --detectOpenHandles",
    "debug": "cross-env DEBUG=true yarn test",
    "prebuild": "yarn -i && yarn clean",
    "build": "parcel build --no-cache ./src/index.html",
    "preserve": "yarn build",
    "serve": "serve ./dist",
    "predeploy": "yarn -i && npm-run-all predeploy:*",
    "predeploy:clean": "yarn clean",
    "predeploy:build": "yarn build --public-url='/puppeteer-examples/'",
    "predeploy:fallbalck": "ncp ./dist/index.html ./dist/404.html",
    "predeploy:nojekyll": "touch ./dist/.nojekyll",
    "deploy": "gh-pages -d ./dist -b gh-pages -m \"Github Pages https://daggerok.github.io/puppeteer-examples/ deployment $(date +%Y-%m-%d)\""
  },
